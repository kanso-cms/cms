{
	"name": "kanso/cms",
	"description": "Kanso Framework",
	"keywords": ["kanso", "framework"],
	"homepage": "https://kanso-cms.github.io",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Joe J. Howard",
			"email": "joejhoward@mail.com"
		},
		{
			"name": "Contributors",
			"homepage": "https://github.com/kanso-cms/cms/graphs/contributors"
		}
	],
	"support": {
		"issues": "https://github.com/kanso-cms/cms/issues",
		"source": "https://github.com/kanso-cms/cms"
	},
	"autoload": {
		"psr-4": {
			"kanso\\": "kanso",
			"app\\": "app"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"kanso\\tests\\": "tests"
		}
	},
	"require": {
		"php": ">=7.0.0",
		"braintree/braintree_php" : "3.40.0",
		"sinergi/browser-detector" : "^6.1",
		"symfony/process": "^4.3",
		"phpmailer/phpmailer": "^6.0",
		"league/oauth2-google": "^3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.5",
		"mockery/mockery": "^1.0",
		"php-mock/php-mock": "^1.0",
		"friendsofphp/php-cs-fixer": "^2.13",
		"phan/phan": "^1.1"
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
